#include <iostream>

using namespace std;

int main() {
    int n=-1;
    cout<<"Vedit 3 znachne chuslo:"<<endl;
    cin>>n;
    if(0>n)cout<<"False, chuslo vidyemne";
    else if(n>=1000)cout<<"False, chuslo 4 znachne";
    else if(n<100)cout<<"False, chuslo 2 znachne";
    else if((n/100)==(n%10))cout<<"True";
    return 0;
}
